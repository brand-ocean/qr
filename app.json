{
  "expo": {
    "name": "NKZW App",
    "slug": "nkzw-app",
    "version": "1.0.0",
    "scheme": "nkzw-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-localization",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Aeonik_Pro/AeonikPro-Regular.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-RegularItalic.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-Bold.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-BoldItalic.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-Light.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-LightItalic.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-Medium.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-MediumItalic.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-Black.otf",
            "./assets/fonts/Aeonik_Pro/AeonikPro-BlackItalic.otf"
          ]
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCanary": true,
      "reactCompiler": true,
      "buildCacheProvider": {
        "plugin": "expo-build-disk-cache"
      },
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.nkzw.www",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.nkzw.www",
      "predictiveBackGestureEnabled": true
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
