    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.2",
    "@rn-primitives/slot": "^1.2.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "expo": "54.0.21",
