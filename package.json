    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "^17.0.7",
    "expo-router": "6.0.14",
    "expo-screen-orientation": "^9.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-video": "^3.0.15",
    "expo-web-browser": "~15.0.8",
    "nativewind": "^4.2.1",
    "react": "^19.2.0",
    "react-native": "~0.81.4",
